{
  "name": "malagasy-tts-extension",
  "displayName": "MalagasyTTS Extension",
  "version": "0.0.1",
  "description": "MalagasyTTS web browser extension",
  "author": "Plasmo Corp. <foss@plasmo.com>",
  "type": "module",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@huggingface/transformers": "github:huggingface/transformers.js#1c19e5ec25d2cd2a919d0e0be9e9340a91770f6b",
    "@mui/material": "^7.0.1",
    "plasmo": "^0.77.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-loading-skeleton": "^3.5.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typedoc": "^0.28.3",
    "typescript": "^5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "contextMenus",
      "storage",
      "activeTab",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self' 'wasm-unsafe-eval'; object-src 'self'"
    }
  }
}
